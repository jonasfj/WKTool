{"name":"Client/Server","model":{"definition":"# Client/Server set-up\nSystem := ClientA | ClientB | Server \\ {request};\n\n# Requests data and unpacks response\nClientA := Client | Client;\nClientB := Client;\nClient := <request!,1>.<response,2>.(failed:<timeout,0>.Client + DeliverData);\n\n#Deliver the data package\nDeliverData := <unpack,5>.<deliver,0>.delivered:Client;\n\n# Server responds or fails\nServer := <request,3>.Response + congested:<timeout!,0>.Server;\n\n#Send response to client\nResponse := <response!,5>.Server;","language":"WCCS"},"properties":[{"state":"System","formula":"E True U[5] failed","comment":"","engine":"Local","encoding":"Symbolic","status":"satisfied"},{"state":"System","formula":"E True U[8] delivered","comment":"","engine":"Local","encoding":"Symbolic","status":"satisfied"},{"state":"System","formula":"E True U[10] (A True U[1] failed)","comment":"","engine":"Local","encoding":"Symbolic","status":"unknown"}]}