h1.test
  text-align:       center
  position:         absolute
  top:              30%
  width:            100%

body
  overflow:         hidden
  font-family:      'Open Sans'

#render-form-actions
  position:         absolute
  bottom:           0px
  margin-bottom:    0px
  width:            100%

#visualization
  display:          none

#render-title
  margin-bottom:    0px

canvas
  cursor:           pointer

.state
  font-family:      Monaco,Menlo,Consolas,"Courier New",monospace

.action
  color:            darkgreen
  font-style:       italic

.props
  color:            blue

#meta-rows
  position:         relative

#targets tr
  cursor:           pointer

#targets tr td
  user-select:      none

#expand-all-warning
  display:          none

.ui-layout-south
  background-color:     white
  margin-bottom:        1px !important

.ui-layout-resizer
  cursor:               row-resize !important
  margin-bottom:        10px !important
  overflow:             visible !important

.ui-layout-center
  overflow:             hidden

.ui-layout-toggler
  opacity:              1 !important

.ui-layout-resizer-sliding
  opacity:              0 !important

.ui-layout-toggler-open
  background-color:     transparent !important
  border-color:         rgb(85,85,85) transparent transparent
  border-style:         solid
  border-width:         12px 20px 0
  height:               0 !important
  width:                0 !important
  margin-left:          10px !important
  top:                  0px !important

.ui-layout-toggler-closed
  background-color:     transparent !important
  border-color:         transparent transparent rgb(85,85,85)
  border-style:         solid
  border-width:         0 20px 12px
  height:               0 !important
  width:                0 !important
  margin-left:          10px !important
  top:                  -6px !important