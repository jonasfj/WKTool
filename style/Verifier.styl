
.form-actions
  margin-bottom:    0px

#property-list
  overflow:         auto
  padding-right:    5px

#edit-property
  overflow:         auto

#verifier
  margin-top:       10px

#properties
  margin-bottom:    10px

#properties tbody tr
  cursor:           pointer

#properties tbody tr:hover
  background:       #e8e8e8


#edit-prop-formula .CodeMirror
  height:           50px

#stop-process
  display:          none

.formula
  text-overflow:    ellipsis
  overflow:         hidden
  max-width:        1px
  white-space:      pre
  font-family:      Monaco,Menlo,Consolas,"Courier New",monospace

td.time
  text-align:       center
  white-space:      pre

#properties tbody tr td
  .icon-question-sign
  .icon-ok
  .icon-remove
  .icon-refresh
    margin-left:    10px

#property-error
  display:        none
  margin-top:     10px
  margin-bottom:  0px

#add-property
  margin-bottom:  10px;

.hidden
  display:        none

#stats-view h3
  margin-top:     0px

.form-horizontal .form-actions
  padding-left:   140px

.form-horizontal .controls
  margin-left:    140px

.form-horizontal .control-label
  width:          120px