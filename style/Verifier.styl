
#property-list
  overflow:         auto
  padding-right:    5px

#verifier
  margin-top:       10px

#properties
  margin-bottom:    10px

#properties tbody tr
  cursor:           pointer

#properties tbody tr:hover
  background:       #e8e8e8


#edit-prop-formula .CodeMirror
  height:           50px

#stop-process
  display:          none

.formula
  text-overflow:    ellipsis
  overflow:         hidden
  max-width:        1px
  white-space:      nowrap

td.time
  text-align:       center

#properties tbody tr td
  .icon-question-sign
  .icon-ok
  .icon-remove
  .icon-refresh
    margin-left:    10px

#property-error
  display:        none
  margin-top:     10px
  margin-bottom:  0px